remove_nas <- function(df, Null, rate=0.5) {
totalRows <- nrow(df)
index = c()
for ( i in colnames(df) ){
found = nrow(df[df[[i]] == Null, ])
if(found >= totalRows*rate){
index <- append(index, c(FALSE))
}
else{ index <- append(index, c(TRUE))}
}
return(index)
}
